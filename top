[32m+[m[32m$(function() {[m
[32m+[m[32m    $('#side-menu').metisMenu();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m//Loads the correct sidebar on window load,[m
[32m+[m[32m//collapses the sidebar on window resize.[m
[32m+[m[32m// Sets the min-height of #page-wrapper to window size[m
[32m+[m[32m$(function() {[m
[32m+[m[32m    $(window).bind("load resize", function() {[m
[32m+[m[32m        var topOffset = 50;[m
[32m+[m[32m        var width = (this.window.innerWidth > 0) ? this.window.innerWidth : this.screen.width;[m
[32m+[m[32m        if (width < 768) {[m
[32m+[m[32m            $('div.navbar-collapse').addClass('collapse');[m
[32m+[m[32m            topOffset = 100; // 2-row-menu[m
[32m+[m[32m        } else {[m
[32m+[m[32m            $('div.navbar-collapse').removeClass('collapse');[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var height = ((this.window.innerHeight > 0) ? this.window.innerHeight : this.screen.height) - 1;[m
[32m+[m[32m        height = height - topOffset;[m
[32m+[m[32m        if (height < 1) height = 1;[m
[32m+[m[32m        if (height > topOffset) {[m
[32m+[m[32m            $("#page-wrapper").css("min-height", (height) + "px");[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    var url = window.location;[m
[32m+[m[32m    // var element = $('ul.nav a').filter(function() {[m
[32m+[m[32m    //     return this.href == url;[m
[32m+[m[32m    // }).addClass('active').parent().parent().addClass('in').parent();[m
[32m+[m[32m    var element = $('ul.nav a').filter(function() {[m
[32m+[m[32m        return this.href == url;[m
[32m+[m[32m    }).addClass('active').parent();[m
[32m+[m
[32m+[m[32m    while (true) {[m
[32m+[m[32m        if (element.is('li')) {[m
[32m+[m[32m            element = element.parent().addClass('in').parent();[m
[32m+[m[32m        } else {[m
[32m+[m[32m            break;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m});[m
